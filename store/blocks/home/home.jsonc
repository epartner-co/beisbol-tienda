{
  "store.home": {
    "props": {
      "blockClass": ["Home"]
    },
    "blocks": [
      "flex-layout.row#TopSlide",
      // "flex-layout.row#Information",
      "flex-layout.row#CategorySlide",
      "rich-text#Title-1",
      "flex-layout.row#Shelf-1",
      "__fold__.experimentalLazyAssets", // ! Lazy Loading /
      "flex-layout.row#Banners",
      "rich-text#Title-2",
      "flex-layout.row#Shelf-2",
      // "flex-layout.row#Banners2",
      // "rich-text#Title-3",
      // "flex-layout.row#Shelf-3",
      "newsletter" // ./newsletter.jsonc
    ]
  },
  // * Top Slide
  "flex-layout.row#TopSlide": {
    "props": {
      "blockClass": ["Home__TopSlide"]
    },
    "children": ["flex-layout.col#TopSlide"]
  },

  // * Information Text
  "flex-layout.row#Information": {
    "props": {
      "blockClass": ["Home__Information"]
    },
    "children": ["flex-layout.col#Information"]
  },

  // * Category Slide
  "flex-layout.row#CategorySlide": {
    "props": {
      "blockClass": ["Home__CategorySlide"]
    },
    "children": ["flex-layout.col#CategorySlide"]
  },

  // *  Title Destacados - 1
  "rich-text#Title-1": {
    "props": {
      "blockClass": [
        "Home__Title",
        "Title--destacados-new"
      ],
      "text": "## Lo m√°s vendido"
    }
  },

  // * Shelf Destacados - 1
  "flex-layout.row#Shelf-1": {
    "props": {
      "blockClass": [
        "Home__Shelf",
        "Home__Shelf--1"
      ]
    },
    "children": ["flex-layout.col#Shelf-1"]
  },

  // * Banners
  "flex-layout.row#Banners": {
    "props": {
      "blockClass": [
        "Home__Banners",
        "Home__Banners--1"
      ]
    },
    "children": ["flex-layout.col#Banners"]
  },

  // *  Title Products - 2
  "rich-text#Title-2": {
    "props": {
      "blockClass": [
        "Home__Title",
        "Title--products"
      ],
      "text": "## Ceras"
    }
  },

  // * Shelf Products - 2
  "flex-layout.row#Shelf-2": {
    "props": {
      "blockClass": [
        "Home__Shelf",
        "Home__Shelf--2"
      ]
    },
    "children": ["flex-layout.col#Shelf-2"]
  },

  // * Banners 2
  "flex-layout.row#Banners2": {
    "props": {
      "blockClass": [
        "Home__Banners",
        "Home__Banners-2"
      ]
    },
    "children": ["flex-layout.col#Banners2"]
  },

  // *  Title Products - 3
  "rich-text#Title-3": {
    "props": {
      "blockClass": [
        "Home__Title",
        "Title--destacados"
      ],
      "text": "## Destacados"
    }
  },

  // * Shelf Products - 3
  "flex-layout.row#Shelf-3": {
    "props": {
      "blockClass": [
        "Home__Shelf",
        "Home__Shelf--3"
      ]
    },
    "children": ["flex-layout.col#Shelf-3"]
  },

